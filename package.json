{
  "name": "indeed-image-convertor",
  "version": "0.0.1",
  "description": "Convert any image to use Indeed-approved colours. (Summer Hackathon 2021)",
  "private": true,
  "homepage": "https://a-ogilvie.github.io/indeed-image-convertor",
  "dependencies": {
    "@types/node": "14.17.19",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "gh-pages": "3.2.3",
    "react": "17.0.2",
    "react-compare-slider": "2.1.0",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "react-scripts": "4.0.3",
    "@zehitomo/eslint-config": "4.7.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "@zehitomo/eslint-config"
    ],
    "rules": {
      "no-shadow": "off",
      "@typescript-eslint/no-shadow": "error",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "hackathon"
  ],
  "contributors": [
    "aogilvie, chung, cyang, mstemm"
  ],
  "license": "ISC",
  "engines": {
    "node": "14",
    "npm": "7"
  }
}
