{
  "name": "indeed-image-convertor",
  "version": "0.0.1",
  "description": "Convert any image to use Indeed-approved colours. (Summer Hackathon 2021)",
  "private": true,
  "homepage": "https://a-ogilvie.github.io/indeed-image-convertor",
  "dependencies": {
    "@types/node": "^18",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "gh-pages": "4.0.0",
    "react": "18.2.0",
    "react-compare-slider": "2.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@a-ogilvie/eslint-config": "5.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "@a-ogilvie",
      "@a-ogilvie/eslint-config/typescript-eslint",
      "@a-ogilvie/eslint-config/react"
    ],
    "rules": {
      "@typescript-eslint/no-shadow": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "no-shadow": "off",
      "no-unused-vars": "off",
      "react/jsx-no-bind": [
        "error",
        {
          "allowArrowFunctions": true
        }
      ]
    },
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "hackathon"
  ],
  "contributors": [
    "aogilvie, chung, cyang, mstemm"
  ],
  "license": "ISC",
  "engines": {
    "node": "18",
    "npm": ">=7"
  },
  "workspaces": [
    "node"
  ],
  "overrides": {
    "jpeg-js": "^0.4.4",
    "minimatch": ">=3.0.5"
  }
}
